{
  "manifest_version": 3,
  "name": "Netflix Advanced Shortcuts",
  "version": "1.1.0",
  "description": "Atalhos W (âˆ’5s) e Q (PT/EN com regra: PT=Pause, EN=Play).",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/watch/*",
        "https://www.netflix.com/*watch*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "page.js"
      ],
      "matches": [
        "https://www.netflix.com/*"
      ]
    }
  ]
}